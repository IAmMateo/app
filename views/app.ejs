<!doctype html>
<html ng-app="likeastore">
<head>
	<meta charset="utf-8">
	<title><%= title %></title>

	<link rel='stylesheet' href='/css/dashboard.css' />
	<%- stylesheets %>

	<!--[if lt IE 9]>
		<script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->
</head>
<body>
	<header class="main-header">
		<div class="cell header-left"><a class="ls-ico ls-logo cell"></a></div>
		<div class="account-info cellRight">
			<span class="account-name"><%= user.username %></span>
			<a class="account-name" href="/logout">logout</a>
		</div>
	</header>

	<nav class="cell dashboard-nav" ng-include src="'partials/nav'"></nav>

	<div ng-view></div>

	<p><a href="/connect/twitter">Connect twitter</a> | <a href="/connect/github">Connect github</a> | <a href="/connect/stackexchange">Connect stackoverflow</a></p>

	<script src="/js/libs/angular/angular.min.js"></script>
	<script src="/js/libs/angular/angular-resource.min.js"></script>
	<script src="/js/app.js"></script>
	<script src="/js/controllers/DashboardCtrl.js"></script>
	<script src="/js/services/api.js"></script>
	<%- scripts %>
</body>
</html>